    <releaseNotes>http://wiki.inkscape.org/wiki/index.php/Release_notes</releaseNotes>
    <!-- ==== Chocolatey Extensions ==== -->
    <packageSourceUrl>https://github.com/WetHat/chocolatey-packages/tree/master/inkscape.portable</packageSourceUrl>
